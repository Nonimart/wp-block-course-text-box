{
	"apiVersion": 2,
	"name": "blocks-cours/text-box",
	"title": "Text Box",
	"category": "text",
	"icon": "text-page",
	"description": "Bloc de Text Box pour le cours Blocks",
	"supports": {
		"html": false
	},
	"keyword": [
		"text-block",
		"text",
		"block"
	],
	"textdomain": "boilerplate",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css",
	"attributes": {
		"text": {
			"type": "string",
			"source": "html",
			"selector": "h4"
		},
		"alignment": {
			"type": "string",
			"default": "left"
		},
		"inputValue": {
			"type": "string"
		},
		"backgroundColor": {
			"type": "string",
			"default": "#f82a2a"
		},
		"textColor": {
			"type": "string",
			"default": "#000"
		},
		"customTextColor": {
			"type": "string"
		},
		"customBackgroundColor": {
			"type": "string"
		}
		
		

	}
}